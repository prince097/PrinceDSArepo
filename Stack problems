# Implemantion using list
stack=[]
stack.append("welcome")
stack.append(2)
stack.append("I am batman")
print(stack)
print(stack.pop())
print(stack)
print(stack.pop())
print(stack)

# Implemantion using deque
from collections import deque
stack=deque()
stack.append("x")
print(stack)
stack.append("y")
stack.append("z")
print(stack)
stack.pop()
print(stack)

# Implemantion using Queue
from queue import LifoQueue
stack=LifoQueue(maxsize=3)
stack.put(10)
stack.put(5)
stack.put(50)
print(stack)
print(stack.qsize())
print(stack.full())
